
## Navigate // Workspaces 1-10, A-Z, ... // <><Ctrl> 0..9, A..Z, ... ##
## Modify // Carry Window to Workspace 1-10, A-Z, ... // <><Ctrl><Shift> 0..9, A..Z, ...  ##
## Launch // Workspace Application A-Z, ... // <><Shift> A..Z, ...  ##

bindsym $mod+Ctrl+1 workspace number 1 1
bindsym $mod+Shift+Ctrl+1 move container to workspace number 1 1; workspace number 1

bindsym $mod+Ctrl+2 workspace number 2 2
bindsym $mod+Shift+Ctrl+2 move container to workspace number 2 2; workspace number 2

bindsym $mod+Ctrl+3 workspace number 3 3
bindsym $mod+Shift+Ctrl+3 move container to workspace number 3 3; workspace number 3

bindsym $mod+Ctrl+4 workspace number 4 4
bindsym $mod+Shift+Ctrl+4 move container to workspace number 4 4; workspace number 4

bindsym $mod+Ctrl+5 workspace number 5 5
bindsym $mod+Shift+Ctrl+5 move container to workspace number 5 5; workspace number 5

bindsym $mod+Ctrl+6 workspace number 6 6
bindsym $mod+Shift+Ctrl+6 move container to workspace number 6 6; workspace number 6

bindsym $mod+Ctrl+7 workspace number 7 7
bindsym $mod+Shift+Ctrl+7 move container to workspace number 7 7; workspace number 7

bindsym $mod+Ctrl+8 workspace number 8 8
bindsym $mod+Shift+Ctrl+8 move container to workspace number 8 8; workspace number 8

bindsym $mod+Ctrl+9 workspace number 9 9
bindsym $mod+Shift+Ctrl+9 move container to workspace number 9 9; workspace number 9

bindsym $mod+Ctrl+0 workspace number 10 0
bindsym $mod+Shift+Ctrl+0 move container to workspace number 10 0; workspace number 10

assign [title=ActivityWatch] workspace number 11 A
bindsym $mod+Ctrl+a workspace number 11 A; exec ~/bin/is_workspace_empty && gtk-launch activitywatch
bindsym $mod+Shift+Ctrl+a move container to workspace number 11 A; workspace number 11

bindsym $mod+Ctrl+b workspace number 12 B; exec ~/bin/is_workspace_empty && google-chrome
bindsym $mod+Shift+Ctrl+b move container to workspace number 12 B; workspace number 12
bindsym $mod+Shift+b exec google-chrome

bindsym $mod+Ctrl+c workspace number 13 C; exec ~/bin/is_workspace_empty && gtk-launch mail
bindsym $mod+Shift+Ctrl+c move container to workspace number 13 C; workspace number 13

bindsym $mod+Ctrl+d workspace number 14 D; exec ~/bin/is_workspace_empty && code
bindsym $mod+Shift+Ctrl+d move container to workspace number 14 D; workspace number 14
bindsym $mod+Shift+d exec code

bindsym $mod+Ctrl+e workspace number 15 E; exec ~/bin/is_workspace_empty && gtk-launch mail
bindsym $mod+Shift+Ctrl+e move container to workspace number 15 E; workspace number 15

bindsym $mod+Ctrl+f workspace number 16 F; exec ~/bin/is_workspace_empty && nautilus --new-window
bindsym $mod+Shift+Ctrl+f move container to workspace number 16 F; workspace number 16
bindsym $mod+Shift+f exec nautilus --new-window

