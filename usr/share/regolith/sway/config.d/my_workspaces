assign [title=Okular] workspace number 28 R
bindsym $mod+Ctrl+r workspace number 28 R; exec ~/bin/is_workspace_empty && flatpak run org.kde.okular
bindsym $mod+Shift+Ctrl+r move container to workspace number 28 R; workspace number 28

bindsym $mod+Ctrl+s workspace number 29 S; exec ~/bin/is_workspace_empty && gnome-control-center
